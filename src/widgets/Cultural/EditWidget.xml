<?xml version="1.0" ?>
<configuration>
	<scripts source="http://geospatialweb.ca/geoJSON/scripts" /> <!-- NO trailing / at end of URL text string -->
	
	<!-- ALL database column names listed throughout MUST conform to the *exact* spelling (including underscores) and capitalization as specified in the database
	     load http://geospatialweb.ca/geoJSON/scripts/getColumnNames.php in your browser to view a list of database column names for reference -->
	
	<!-- DropDownList can accommodate maximum of 12 characters for field name -->
	<search>
		<field name="Name" />
		<field name="SubCategory" />
		<field name="Keywords" />
		<field name="Address" />
        <field name="City" />
        <field name="Phone" />
    </search>
    
    <!-- column names of database geographic coordinates -->
	<coords lat="Latitude" lng="Longitude" />
	
	<!-- column name of database spatial geometry field -->
	<geometryfield>geom</geometryfield>
	
	<!-- database field value displayed above the horizontal rule in the InfoWindow PopUp -->
	<titlefield>Name</titlefield>
	
	<!-- column name of database primary key -->
	<primarykey>ID</primarykey>
</configuration>